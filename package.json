{
  "name": "crazyrydmi.github.io",
  "version": "0.1.0",
  "description": "My personal home page",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline",
    "test": "echo \"Error: no test specified\" && exit 1",
    "images": "cpy src/images/* dist/images",
    "glsl": "cpy src/glsl/* tsoutput/glsl",
    "css": "cpy src/css/app.css dist --rename=bundle.css",
    "compress": "uglifyjs dist/bundle.js -o dist/bundle.js",
    "browserify": "md dist & npm run glsl && browserify -t glslifyify -e tsoutput/ts/app.js -o dist/bundle.js --debug",
    "tslint": "tslint src/ts/**.ts",
    "babel": "babel tsoutput/ts --out-dir tsoutput/ts ",
    "debug": "tsc && npm run tslint && npm run babel && npm run browserify && npm run css",
    "release": "npm run debug && npm run compress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CrazyRyDmi/CrazyRyDmi.github.io.git"
  },
  "keywords": [
    "homepage"
  ],
  "author": "Rybak Dmitry <rydmi@yandex.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CrazyRyDmi/CrazyRyDmi.github.io/issues"
  },
  "homepage": "https://github.com/CrazyRyDmi/CrazyRyDmi.github.io#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "browserify": "^13.0.1",
    "concat-cli": "^4.0.0",
    "copy-webpack-plugin": "^3.0.0",
    "cpy-cli": "^1.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "glsl-transition-minify": "^0.3.1",
    "glslifyify": "^0.1.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-file": "^0.3.0",
    "gulp-foreach": "^0.1.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "minifyify": "7.3.3",
    "postcss": "^5.0.21",
    "stream-buffers": "^3.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tsify": "^0.15.5",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "1.0.4",
    "uglifyjs": "^2.4.10",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-glsl-loader": "^1.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "babel-runtime": "^6.6.1",
    "systemjs": "^0.19.27",
    "three": "^0.75.0"
  }
}
